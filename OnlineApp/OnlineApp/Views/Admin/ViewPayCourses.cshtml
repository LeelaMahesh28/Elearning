<!DOCTYPE html>
<html>

<body>
    @model IEnumerable<OnlineApp.Models.PayDetail>
    @{
        ViewBag.Title = "ViewRegCourses";
    }

    <h2>Payment Details</h2>

    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(Pay => Pay.PaymentId)
            </th>
            <th>
                @Html.DisplayNameFor(Pay => Pay.Uid)
            </th>
            <th>
                @Html.DisplayNameFor(Pay => Pay.Cid)
            </th>
            <th>
                @Html.DisplayNameFor(Pay => Pay.Status)
            </th>
            <th>
                @Html.DisplayNameFor(Pay => Pay.ScheduleDate)
            </th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(m => item.PaymentId)
                </td>
                <td>
                    @Html.DisplayFor(m => item.Uid)
                </td>
                <td>
                    @Html.DisplayFor(m => item.Cid)
                </td>
                <td>
                    @Html.DisplayFor(m => item.Status)
                </td>
                <td>
                    @Html.DisplayFor(m => item.ScheduleDate)
                </td>
            </tr>
        }
    </table>
    <div>
        @Html.ActionLink("Back to List", "ViewAdminCourses")
    </div>
    <style>
        body {
            background-image: url('data:image/jpeg;base64,');
            background-repeat: no-repeat;
            background-attachment: fixed;
            background-size: 100% 100%;
            opacity:1.5;
            text-align: center;
            color: white;
            font-weight: bolder;
            font-family: cursive;
        }

        table {
            border-collapse: collapse;
            width: 100%;
        }

        th {
            text-align: left;
            padding: 8px;
            background-color: deeppink;
            color: white;
        }

        tr:nth-child(even) {
            background-color: hotpink;
            color: black;
        }

        tr:nth-child(odd) {
            background-color: white;
            color: black;
        }
        </style >
    </body>
</html>